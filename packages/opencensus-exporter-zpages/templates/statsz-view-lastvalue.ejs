<table cellspacing="0" cellpadding="0">
    <thead>
        <tr>
        <? for (var tagKey of statsData[0].tagKeys) { ?>
            <th class="borderRL">TagKey: <?= tagKey ?></th>                
        <? } ?>
            <th class="borderRL">Last Value, <?= view.measure.unit ?></th>
        </tr>
    </thead>
    <tbody>
        <? if (statsData[0].tagValues.length > 0) { ?>
            <? for (var data of statsData) { ?>
                <tr>
                    <? for (var tagValue of data.tagValues) { ?>
                        <td class="borderRL"><?= tagValue ?></td>
                    <? } ?>            
                <td class="borderRL"><?= data.snapshot.value ?></td>
                </tr>
            <? } ?>
        <? } ?>
    </tbody>
</table>
